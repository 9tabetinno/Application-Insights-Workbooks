    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "union withsource=['AzureDiagnostics'] *\r\n| where Category =~ 'MySqlSlowLogs'\r\n| where columnifexists(\"query_time_d\",0) > 10\r\n| project TimeGenerated,Resource, columnifexists(\"event_class_s\",\"\"), columnifexists(\"start_time_t\",\"\") , columnifexists(\"query_time_d\",\"\"), columnifexists(\"sql_text_s\",\"\") \r\n| summarize count() by Resource , bin(TimeGenerated, 5m)\r\n| render timechart\r\n",
        "size": 0,
        "timeContext": {
          "durationMs": 86400000
        },
        "queryType": 0,
        "resourceType": "microsoft.dbformysql/flexibleservers",
        "visualization": "barchart"
      },
      "conditionalVisibility": {
